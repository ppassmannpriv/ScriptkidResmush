<?xml version="1.0"?>
<config>
  <modules>
    <Scriptkid_Resmush>
      <version>0.1.0</version>
    </Scriptkid_Resmush>
  </modules>
  <global>
    <helpers>
      <resmush>
        <class>Scriptkid_Resmush_Helper</class>
      </resmush>
    </helpers>
    <models>
      <resmush>
        <class>Scriptkid_Resmush_Model</class>
      </resmush>
    </models>
  </global>
  <adminhtml>
    <events>
      <catalog_product_gallery_upload_image_after>
        <observers>
          <resmush>
            <class>resmush/observer</class>
            <method>catalogProductGalleryUploadImageAfter</method>
          </resmush>
        </observers>
      </catalog_product_gallery_upload_image_after>
    </events>
  </adminhtml>
  <admin>
    <routers>
      <adminhtml>
        <args>
          <modules>
            <resmush before="Mage_Adminhtml">Scriptkid_Resmush_Adminhtml</resmush>
          </modules>
        </args>
      </adminhtml>
    </routers>
  </admin>

</config>
